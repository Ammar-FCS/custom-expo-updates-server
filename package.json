{
  "name": "custom-expo-updates-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "eslint .",
    "expo-publish": "cd client && expo export --experimental-bundle && cd ../ && rm -rf updates/1/ && cp -r client/dist/ updates/1",
    "expo-publish-test": "cd client && expo export --experimental-bundle && cd ../ && rm -rf updates/test/ && cp -r client/dist/ updates/test",
    "custom-script": "ts-node -r tsconfig-paths/register --project tsconfig.scripts.json ./scripts/run.ts"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "mime": "^2.5.2",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.14.5",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/mime": "^2.0.3",
    "@types/node-forge": "^0.10.8",
    "@types/react": "^17.0.32",
    "babel-jest": "^26.6.3",
    "eslint": "^7.15.0",
    "eslint-config-universe": "^8.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "node-forge": "^0.10.0",
    "node-mocks-http": "^1.10.1",
    "prettier": "~2.0.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
